<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="oval">
            <corners android:radius="12dp" />
            <solid android:color="@color/themeColorLight" />
            <padding android:bottom="12dp" android:top="12dp" android:left="12dp" android:right="12dp" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/themeColorPrimary" />
            <padding android:bottom="12dp" android:top="12dp" android:left="12dp" android:right="12dp" />
        </shape>
    </item>
</selector>